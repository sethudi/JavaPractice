package application;


public class App {

	public static void main(String[] args) {
		int[] tray;
		tray = {4,5,3};
		System.out.println(tray);		
		int[] numbers = new int[3];
		
		numbers[0] = 5;
		numbers[1] = 123;
		numbers[2] = 3;
		
		for(int i=0; i < 3; i++) {
			System.out.println(numbers[i]);
		}
		
	}

}